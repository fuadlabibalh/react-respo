{
  "name": "react-resp-rr",
  "version": "1.0.3",
  "description": "react responsive",
  "author": "fuadlabibalh",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build:cjs": "tsc --outDir ./dist/cjs --module commonjs --declarationDir ./dist/cjs",
    "build:esm": "tsc --outDir ./dist/esm --module esnext --declarationDir ./dist/esm",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "devDependencies": {
    "@types/react": "^18.1.1",
    "@types/react-dom": "^18.1.1",
    "react": "^18.1.1",
    "react-dom": "^18.1.1",
    "typescript": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^18.1.1",
    "react-dom": "^18.1.1",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fuadlabibalh/react-respo.git"
  },
  "dependencies": {
    "react-resp-rr": "^1.0.2"
  }
}
